<div>
	<h1>PRODUCTS</h1>

	<table>
		{{#each products}}
		<tr>
			<td>{{this.id}}</td>
			<td>{{this.title}}</td>
			<td>{{this.description}}</td>
			<td>{{this.category}}</td>
			<td>{{this.price}}</td>
			<td>{{this.thumbnail}}</td>
			<td>{{this.code}}</td>
			<td>{{this.stock}}</td>
			<td>{{this.status}}</td>
		</tr>
		{{/each}}
	</table>

</div>
